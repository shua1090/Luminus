function isPrime(int num) returns int{
    int ctr = 2;
    if (num == 2){
        return 1;
    }
    if (num <= 1){
        return 0;
    }
    while ctr < (num/2) {
        if ( num % ctr == 0){
            printf("%d is divisible by %d\n", num, ctr);
            return 0;
        }
        ctr = ctr + 1;
    }
    return 1;
}