func main() -> int {
    int array [20];
    int size = 20;
    int i = 0;



    while i < size{
        array[i] = (i+1) * (i+1);
        i = i + 1;
    }
    i = 0;
    printf("Finished calculating squares\n");
    while i < size {
        printf("%d squared is %d\n", (i+1), array[i]);
        i = i + 1;
    }

    return 0;
}